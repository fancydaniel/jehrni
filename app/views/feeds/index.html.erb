<% if current_user %>
<div id="feed">
  <h2>My Feed Page</h2> <br />
    <h5 class=""><%= link_to "New", new_feed_path %></h5>


      <% @feeds.each do |feed| %>
        <p>
          <%= link_to feed.url, feed%>
          <%= link_to "Edit", edit_feed_path(feed) %> 
          <%= link_to "Delete", feed_path(feed), method: :delete, data: { confirm: 'Are you sure you want to delete this feed url?' } %>
        </p>
        <% feed.entries.each do |entry| %>
          <p><%= entry.title %></p>
            <p><%= sanitize entry.summary,  length: 30 %></p>
        <% end %> <br />
      <% end %>

</div>
<% else %>
  <div id="feed">
  <h2> Not authorized </h2>
  <h5> Please register for your own personal feed </h5>
  </div>
<% end %>


